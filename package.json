{
	"name": "directus-extension-parser",
	"description": "This extension allow you to parse a string (for ex: key1=value1;key2=value2;) so that update keys and values is much easier.",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-interface"
	],
	"type": "module",
	"directus:extension": {
		"type": "interface",
		"path": "dist/index.js",
		"source": "src/index.js",
		"host": "^10.3.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "10.3.0",
		"vue": "^3.4.11"
	}
}
